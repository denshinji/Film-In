<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.favmov">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_fav"
        android:label="FAVMOV"
        android:roundIcon="@drawable/icon_fav"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.example.filmin.READ_DATABASE"/>
    <uses-permission android:name="com.example.filmin.WRITE_DATABASE"/>

</manifest>